// Problem number 3
#include<stdio.h>
void display(int i, int n)
{
    if(i>n) return;
    printf("%d ",i);
    display(i+1, n);
}
int main(void)
{
    int n;
    scanf("%d",&n);
    display(1,n);
    return 0;
}
