// Problem number 5
#include<stdio.h>
int add_num(int i, int n)
{
    if(i>n) return 0;
    int sum= add_num(i+1, n);
    return sum+i;
}
int main(void)
{
    int n;
    scanf("%d",&n);
    int ans= add_num(1,n);
    printf("%d",ans);

    return 0;
}
