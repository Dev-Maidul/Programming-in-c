// Problem number 1
#include<stdio.h>
int power(int n, int m)
{
    if(m<1) return 1;
    int sum= power(n, m-1);
    return n*sum;
}
int main(void)
{
    int n,m;
    scanf("%d%d",&n,&m);
    int ans= power(n,m);
    printf("%d",ans);

    return 0;
}
