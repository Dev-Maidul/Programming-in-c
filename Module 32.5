// Problem number 1
#include<stdio.h>
int power(int n, int m)
{
    if(m<1) return 1;
    int sum= power(n, m-1);
    return n*sum;
}
int main(void)
{
    int n,m;
    scanf("%d%d",&n,&m);
    int ans= power(n,m);
    printf("%d",ans);

    return 0;
}
// Problem number 2
#include<stdio.h>
int add_num(int ara[], int i, int n)
{
   if(i==n) return 0;
   int s=  add_num( ara, i+1, n);
   return s+ ara[i];
}
int main(void)
{
    int n;
    scanf("%d",&n);
    int ara[n];
    for(int i=0; i<n; i++)
    {
        scanf("%d",&ara[i]);
        ara[i]=ara[i]%10;
    }
   int ans=add_num(ara,0,n);
    printf("%d",ans);

    return 0;
}


// Problem number 3
#include<stdio.h>
void display(int i, int n)
{
    if(i>n) return;
    printf("%d ",i);
    display(i+1, n);
}
int main(void)
{
    int n;
    scanf("%d",&n);
    display(1,n);
    return 0;
}
