#include <stdio.h>
int main(void)
{
  FILE *input_file;
  input_file = fopen("input.txt", "r");
  if (input_file == NULL)
  {
    printf("File not found");
    return 0;
  }
  FILE *output_file;
  output_file = fopen("output.txt", "w");
  int t;
  fscanf(input_file, "%d", &t);
  while (t--)
  {
    int n, i;
    fscanf(input_file, "%d", &n);
    if (n > 0)
    {
      for (i = n; i >= -n; i--)
      {
        fprintf(output_file, "%d ", i);
        fprintf(output_file, "\n");
        
      }
    }
   
    else
    {
      for (i = n; i <= (-n); i++)
      {
        // print i
        fprintf(output_file,"%d ",i);
      }
    }
  }

  return 0;
}
